[core]
    excludesfile = ~/.gitignore
    fileMode = false
    autocrlf = input
[push]
    default = matching
[pull]
    default = current
[branch]
    autosetuprebase = always
[alias]
    s = status
    ci = commit
    br = branch
    co = checkout
    aa = add --all
    rev = remote -v
    pull = pull --rebase
    logg = log --graph --oneline --decorate --all
    logn = log --stat --graph --oneline --decorate --all
    diffn = diff --name-only

